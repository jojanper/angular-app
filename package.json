{
  "name": "angular-app",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojanper/angular-app.git"
  },
  "bugs": {
    "url": "https://github.com/jojanper/angular-app/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run worker-dev-build && ng serve",
    "build": "npm run worker-prod-build && ng build --prod --delete-output-path=false",
    "prod-watch": "npm run worker-prod-build && ng build --prod --watch=true --delete-output-path=false",
    "dev-watch": "npm run worker-dev-build && ng build --watch=true",
    "eslint": "eslint '*worker*.js' './src/app/**/*.js'",
    "test": "ng test --watch=false && npm run worker-unittest",
    "test-coverage": "ng test --watch=false --code-coverage",
    "test-watch": "ng test --watch=true --single-run=false",
    "lint": "npm run eslint && ng lint",
    "e2e": "ng e2e",
    "cibuild-travis": "npm run lint && npm test -- --environment=travis",
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "sloc": "sloc src",
    "http-server": "http-server",
    "serve-build": "npm run http-server -- dist --p 4200",
    "worker-prod-build": "NODE_ENV=production ng run worker-app:build",
    "worker-dev-build": "ng run worker-app:build",
    "worker-unittest": "NODE_ENV=test karma start karma.worker.conf.js",
    "gh-pages": "rm -Rf dist && mkdir -p dist && npm run gh-pages-build && npm run gh-pages-deploy",
    "gh-pages-build": "npm run build -- --deploy-url /angular-app/ --base-href /angular-app/",
    "gh-pages-deploy": "cd dist && git init && git add -A && git commit -m \"deploy\" && git push -f git@github.com:jojanper/angular-app.git master:gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/router-store": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@nguniversal/express-engine": "^7.0.0",
    "angular-router-loader": "0.8.5",
    "chosen-js": "1.8.3",
    "core-js": "^2.5.7",
    "datatables.net": "^1.10.19",
    "datatables.net-bs": "^1.10.19",
    "jquery": "^3.3.1",
    "ngx-cookie": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.2",
    "simple-peer": "^9.1.2",
    "socket.io": "~2.1.1",
    "xterm": "^3.14.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.1",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@types/chosen-js": "^1.8.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.3.2",
    "@types/node": "^10.12.18",
    "@types/socket.io-client": "~1.4.32",
    "@types/xterm": "^3.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.1",
    "codecov": "^3.0.2",
    "codelyzer": "^5.0.1",
    "cors": "^2.8.5",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "http-server": "latest",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "protractor": "^5.3.2",
    "rimraf": "^2.6.2",
    "serve-static-throttle": "^3.0.2",
    "shelljs": "^0.8.2",
    "sloc": "^0.2.0",
    "ts-node": "~4.1.0",
    "tslint": "^5.10.0",
    "typescript": "~3.4.5"
  },
  "engines": {
    "node": ">= 8.4.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "object-curly-spacing": 0,
      "indent": [
        "error",
        4
      ],
      "import/no-extraneous-dependencies": [
        0
      ],
      "import/no-unresolved": [
        0
      ],
      "import/prefer-default-export": [
        0
      ],
      "max-len": [
        "error",
        {
          "code": 135
        }
      ]
    },
    "env": {
      "node": true,
      "jasmine": true
    },
    "globals": {
      "describe": true,
      "expect": true,
      "Worker": true
    }
  }
}
